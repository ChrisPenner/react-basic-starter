{
  "name": "react-basic-starter",
  "version": "0.1.0",
  "description": "React Basic documentation and starter project",
  "main": "src/index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumihq/react-basic-starter.git"
  },
  "keywords": [
    "react",
    "react-basic",
    "react-basic-classic",
    "react-basic-hooks",
    "hooks",
    "purescript"
  ],
  "author": "Madeline Trotter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lumihq/react-basic-starter/issues"
  },
  "homepage": "https://github.com/lumihq/react-basic-starter#readme",
  "scripts": {
    "build": "spago build; true > output/Main-bundled.js",
    "bundle:parcel": "rm -rf dist; parcel build src/index.html --public-url ./",
    "bundle:purs": "spago bundle-module --main Main --to output/Main-bundled.js",
    "bundle": "npm run bundle:purs && npm run bundle:parcel",
    "clean": "rm -rf output node_modules .spago .cache dist",
    "deploy": "gh-pages -d dist",
    "prebundle": "npm run build",
    "predeploy": "npm run bundle",
    "prestart": "npm run build",
    "pscid": "pscid",
    "start": "parcel start src/index.html",
    "test": "spago test"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "highlight.js": "^10.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "gh-pages": "^3.0.0",
    "parcel-bundler": "^1.12.4",
    "purescript": "^0.13.6",
    "spago": "^0.15.2"
  }
}
